<h1> Hello and Welcome </h1>
<br>
<% if current_user %>

  <%= link_to 'Logout', logout_path, :data => { :confirm => 'Are you sure?' }%>
<% else %>
  <%= link_to 'Signup', signup_path %>
  <%= link_to 'Login', '/login'%>
<% end %>

 <% @users.each do |user| %>
   <li>
     <a href="/users/<%= user.id %>">  
     <%= user.id %>  
     <%= user.name %>
   </li>
  <% end %>
 </ul>

